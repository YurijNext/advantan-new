header#header.b-header
    .container
        .b-header__wrap
            .b-header__logo
                a.b-header__logo-link(href="/")
                    img.b-header__logo-img(src="img/advantanLogo.svg" alt="Logo")

            nav.b-header__nav
                ul.b-header__nav-list
                    li.b-header__nav-item
                        a.b-header__nav-link.b-header__nav-link--active(href="index.html/#brand") О бренде
                    li.b-header__nav-item
                        a.b-header__nav-link(href="index.html") Видео
                    li.b-header__nav-item
                        a.b-header__nav-link(href="index.html") Тест
                    li.b-header__nav-item
                        a.b-header__nav-link(href="index.html") Статьи

            .b-header__socials
                ul.b-header__socials-list
                    li.b-header__socials-item
                        a.b-header__socilas-link(href="https://www.instagram.com/mustela_russia/" target="_blank")
                            svg.b-header__socilas-svg(xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' width='25' height='25' viewbox='0 0 25 25')
                                    g
                                        image(width='25' height='25' xlink:href='data:image/png;base64,')
                    li.b-header__socials-item
                        a.b-header__socilas-link(href="https://vk.com/mustelarussia" target="_blank")
                            svg.b-header__socilas-svg(xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' width='32' height='19' viewbox='0 0 32 19')
                                    g
                                        image(width='32' height='19' xlink:href='data:image/png;base64,')

            button.hamburger.b-header__humburger(type='button')
                span.hamburger-box.b-header__humburger-box
                    span.hamburger-inner.b-header__humburger-inner

    .b-header__right-bg

    include ../blocks/_mob-menu


